#!/bin/sh
#
# list quotas for global disks
#
fsqstring=`fsquota`

echo "$fsqstring" | grep 'FILESETS FOR:' | tail -1 |\
  sed -e"s/          FILESETS FOR:.*$/FILESETS FOR:/" | tr "\n" " "
echo "$fsqstring" | grep 'FILESETS FOR:' |\
  sed -e"s/^.*FILESETS FOR://" | tr "\n" " "
echo
echo "$fsqstring" | grep 'Mount Point' | tail -1
echo "$fsqstring" | grep -e '^scrub' -e global | sort -k7nr

